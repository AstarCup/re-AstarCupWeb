model MultiplayerTeamvsRoom {
    room_id          Int               @id @default(autoincrement())
    team_red_id      Int               @unique
    team_blue_id     Int               @unique
    team_red         Team              @relation("red_team", fields: [team_red_id], references: [id])
    team_blue        Team              @relation("blue_team", fields: [team_blue_id], references: [id])
    multiplayerRooms MultiplayerRoom[]
}
