model UserGroup {
    id        Int           @id @default(autoincrement())
    group     UserGroupType @default(PLAYER)
    users     User[]
    createdAt DateTime      @default(now())
    updatedAt DateTime      @updatedAt
}

enum UserGroupType {
    HOST
    ADMIN
    POOLER
    STREAMER
    TESTER
    GFX
    SHEETER
    COMMENTATOR
    MAPPER
    REFEREE
    PLAYER
}
